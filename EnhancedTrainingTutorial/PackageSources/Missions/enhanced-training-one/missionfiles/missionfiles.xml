        <SimMission.Tutorial InstanceId="{EA8E6426-13D4-4CE8-8EBC-0E94E78DEA3A}" id="tutorial1">
            <Descr>Prop Lever training</Descr>
            <SuccessText>TT:MISSIONMESSAGE.ACTIVITY.SUCCESS</SuccessText>
            <FailureText>TT:MISSIONMESSAGE.ACTIVITY.FAILED</FailureText>
            <AbortedText>TT:MISSIONMESSAGE.ACTIVITY.ABORTED</AbortedText>
            <Objectives>
                <Objective UniqueRefId="{F1415134-4F77-4CCF-89B4-70241725FBBD}" id="OBJ_ListenToIntro">
                    <Descr>Learn about the Prop Lever</Descr>
                    <SuccessText>Success</SuccessText>
                    <Steps>
                        <Step UniqueRefId="{F3EF448F-E9DE-4BB8-9347-AC3C9B73D5E0}" id="ST_ListenToIntro">
                            <Descr>Listen to the introduction</Descr>
                        </Step>
                    </Steps>
                </Objective>
                <Objective UniqueRefId="{4177AC50-ADC6-4FB7-9D9E-B58119E714F9}" id="OBJ_CompleteRunupTest">
                    <Descr>Exercise the prop lever for run-up</Descr>
                    <Steps>
                        <Step UniqueRefId="{3A713B82-710D-4E33-AAFE-1E517E12865C}" id="ST_IncreaseThrottle">
                            <Descr>Increase throttle to set RPM to 1600</Descr>
                            <SuccessText>Complete</SuccessText>
                        </Step>
                        <Step UniqueRefId="{DBE49600-6C8A-4599-B4CF-CCC4E9D21920}" id="ST_DecreasePropLever">
                            <Descr>Pull back the Prop Lever fully</Descr>
                        </Step>
                        <Step UniqueRefId="{80915729-2EA8-4FB1-81C2-0AECA3CFC6B7}" id="ST_IncreasePropLever">
                            <Descr>Push the Prop Lever back to full RPM</Descr>
                        </Step>
                        <Step id="ST_ReturnToIdle" UniqueRefId="{7B756F41-A97A-4DEE-940B-DF58920291FE}">
                            <Descr>Bring throttle back down to idle</Descr>
                        </Step>
                    </Steps>
                </Objective>
                <Objective UniqueRefId="{EFBCCE4C-6091-4BAB-846B-FF6D1395AAED}" id="OBJ_TaxiTakeoff">
                    <Descr>Taxi and Takeoff from Runway 25</Descr>
                    <Steps>
                        <Step UniqueRefId="{3957C704-2C45-44DD-8DAE-A7B7B93442B5}" id="ST_Taxi">
                            <Descr>Taxi and Take Off Runway 25</Descr>
                            <SuccessText>Complete</SuccessText>
                        </Step>
                    </Steps>
                </Objective>
                <Objective UniqueRefId="{045BA93C-717D-482D-B94D-2A7CD48736E1}" id="OBJ_FlyToLompoc">
                    <Descr>Navigate to Lompoc</Descr>
                    <Steps>
                        <Step UniqueRefId="{C20BA090-4A71-4A0C-A4C1-B8D81812B27A}" id="ST_RetractGear">
                            <Descr>Retract Landing Gear</Descr>
                        </Step>
                        <Step id="ST_LiftFlaps" UniqueRefId="{ABBD4C61-7401-44FE-A984-49BAF54E4391}">
                            <Descr>Lift Flaps</Descr>
                        </Step>
                        <Step UniqueRefId="{F8926B95-C005-4777-BE40-EE0F4496DEE2}" id="ST_ClimbToCruise">
                            <Descr>Climb to 3,500 ft</Descr>
                        </Step>
                        <Step UniqueRefId="{9B3239BC-9829-4755-93DF-EBD1DFB6B5C6}" id="ST_SetCourseForLompoc">
                            <Descr>Follow heading 283 to Lompoc</Descr>
                        </Step>
                        <Step UniqueRefId="{27D4E499-8754-486A-8AFB-2EC0F8FDEA4E}" id="ST_ReducePower18">
                            <Descr>Reduce Throttle to 18 inches</Descr>
                        </Step>
                        <Step UniqueRefId="{E5183CF1-3380-4BF5-947C-8694F83478D2}" id="ST_ReduceThrottle1800">
                            <Descr>Reduce Throttle to reach 1800</Descr>
                        </Step>
                    </Steps>
                </Objective>
            </Objectives>
        </SimMission.Tutorial>
